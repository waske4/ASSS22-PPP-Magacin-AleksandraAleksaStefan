program Project1;

uses
  Vcl.Forms,
  frmMain_u in 'frmMain_u.pas' {frmMain},
  frmIntro_u in 'frmIntro_u.pas' {frmIntro},
  frmSklRobe_u in 'frmSklRobe_u.pas' {frmSklRobe},
  frmSklProizvoda_u in 'frmSklProizvoda_u.pas' {frmSklProizvoda};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmMain, frmMain);
  Application.CreateForm(TfrmSklRobe, frmSklRobe);
  Application.CreateForm(TfrmSklProizvoda, frmSklProizvoda);
  Application.Run;
end.
